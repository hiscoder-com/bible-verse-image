{
  "name": "@texttree/bible-verse-image",
  "author": "TextTree.org",
  "version": "0.1.1",
  "description": "",
  "keywords": [
    "rcl",
    "react",
    "canvas",
    "image"
  ],
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "url": "https://github.com/texttree/bible-verse-image",
  "homepage": "https://github.com/texttree/bible-verse-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/texttree/bible-verse-image.git"
  },
  "bugs": {
    "url": "https://github.com/texttree/bible-verse-image/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ],
    "*.+(json|md)": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "styleguidist build",
    "start": "styleguidist server",
    "lint": "eslint --fix .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "shx rm -rf ./dist & babel ./src --out-dir ./dist -s inline",
    "prettier": "prettier --write .",
    "build:babel": "babel ./src --out-dir ./dist -s inline --ignore './src/components/**/*.jsx'",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-env": "^7.22.14",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.1",
    "shx": "^0.3.4",
    "standard-version": "^9.5.0",
    "dotenv-webpack": "^8.0.1",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "unsplash-js": "^6.3.0"
  }
